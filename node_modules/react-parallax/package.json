{
    "name": "react-parallax",
    "author": "Richard Rutsche",
    "version": "2.2.0",
    "description": "A React Component for parallax effect working in client-side and server-side rendering environment",
    "repository": {
        "type": "git",
        "url": "https://github.com/RRutsche/react-parallax.git"
    },
    "main": "dist/index.js",
    "scripts": {
        "test": "jest",
        "test:watch": "npm run test -- --watch",
        "test:cover": "npm run test -- --coverage",
        "dev": "NODE_ENV=development && webpack-dev-server --mode development --config webpack.config.dev.js",
        "dist": "NODE_ENV=production && webpack --mode production --config webpack.config.build.js",
        "lint": "eslint src",
        "format": "prettier --write \"*/**/*.{js,json,css,scss}\" \"*.js\""
    },
    "keywords": [
        "react",
        "reactjs",
        "parallax",
        "scroll",
        "jsx"
    ],
    "peerDependencies": {
        "prop-types": "^15.5.8",
        "react": "^0.14.9 || ^15.3.0-0 || 16.x.x",
        "react-dom": "^0.14.9 || ^15.3.0-0 || 16.x.x"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.3.3",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer-loader": "^3.2.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.1.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-react-transform": "^3.0.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-runtime": "^6.23.0",
        "css-loader": "^2.1.0",
        "eslint": "^5.14.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-destructuring": "^2.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jest": "^22.3.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.1.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^3.0.1",
        "fsevents": "^1.1.3",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "imports-loader": "^0.8.0",
        "jasmine-core": "^3.3.0",
        "jest": "^24.1.0",
        "jscs": "^3.0.7",
        "jsdom": "^13.2.0",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-jasmine": "~2.0.1",
        "lint-staged": "^8.1.4",
        "node-libs-browser": "^2.0.0",
        "node-sass": "^4.5.2",
        "prettier": "^1.7.4",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^16.0.0",
        "react-hot-loader": "^4.6.5",
        "react-router-dom": "^4.3.1",
        "sass-loader": "^7.1.0",
        "script-loader": "^0.7.0",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.4",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.1.14",
        "webpack-merge": "^4.1.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "post-merge": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.js": [
            "prettier --write",
            "eslint",
            "git add"
        ],
        "*.{json,css,scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/setupFile.js"
        ]
    }
}
